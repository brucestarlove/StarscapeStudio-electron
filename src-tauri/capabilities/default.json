{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:event:allow-emit",
    "core:path:default",
    "opener:default",
    "fs:default",
    "fs:read-file",
    "fs:write-file",
    "fs:create-dir",
    "fs:read-dir",
    "fs:copy-file",
    "fs:move-file",
    "fs:remove-file",
    "fs:remove-dir",
    "fs:exists",
    "fs:scope:allow-read",
    "fs:scope:allow-write"
  ]
}
